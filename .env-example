# URL de conexão com o banco de dados PostgreSQL (NeonDB)
DATABASE_URL="postgresql://USER:PASSWORD@HOST/DATABASE_NAME?sslmode=require"

# Credenciais para autenticação com o Google OAuth
GOOGLE_CLIENT_ID="your_google_client_id_here"
GOOGLE_CLIENT_SECRET="your_google_client_secret_here"

# URL base para o NextAuth (autenticação)
NEXTAUTH_URL="http://localhost:3000"

# Chave secreta usada pelo NextAuth para criptografia
NEXTAUTH_SECRET="your_nextauth_secret_here"
